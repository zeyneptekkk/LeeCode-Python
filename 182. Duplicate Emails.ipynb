    "import pandas as pd\n",
    "\n",
    "def duplicate_emails(person: pd.DataFrame) -> pd.DataFrame:\n",
    "    tekrar_eden_mail=person[person[\"email\"].duplicated(keep=\"first\")]\n",
    "\n",
    "    tekrar_eden_mail=tekrar_eden_mail.drop(\"id\",axis=1)\n",
    "    \n",
    "    return tekrar_eden_mail"
   ]
  }

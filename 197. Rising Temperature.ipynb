    "import pandas as pd\n",
    "\n",
    "def rising_temperature(weather: pd.DataFrame) -> pd.DataFrame:\n",
    "    \n",
    "   \n",
    "    \n",
    "    weather[\"önceki_gün\"]=weather[\"temperature\"].shift(1)\n",
    "    weather_sonuc=weather[weather[\"önceki_gün\"]<weather[\"temperature\"]]\n",
    "\n",
    "    dff=weather_sonuc[[\"id\"]]\n",
    "    return dff"
   ]
  }

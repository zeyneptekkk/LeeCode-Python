    "class Solution(object):\n",
    "    def addBinary(self, a, b):\n",
    "       num1=int(a,2)   # burdaki 2 , ikili (binary) sayı sisteminde (taban 2) olduğunu belirtir\n",
    "       num2=int(b,2)   \n",
    "       sum=num1+num2\n",
    "       binary_sum=bin(sum)[2:]\n",
    "       return binary_sum\n"
   ]
  }
